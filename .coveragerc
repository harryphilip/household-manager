[run]
source = household
omit = 
    */migrations/*
    */tests/*
    */venv/*
    */env/*
    manage.py
    */settings.py
    */__init__.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov

